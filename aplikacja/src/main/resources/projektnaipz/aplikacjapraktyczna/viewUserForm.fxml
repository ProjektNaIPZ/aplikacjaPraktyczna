<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projektnaipz.aplikacjapraktyczna.JavaFxController.ViewUserForm">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="title" graphicTextGap="1.0" />
    <ScrollPane id="scrollPane" hbarPolicy="NEVER" prefHeight="430.0" prefWidth="760.0" vbarPolicy="ALWAYS">
        <content>
            <VBox fx:id="vbox" prefWidth="742.0" spacing="10.0" />
        </content>
    </ScrollPane>
    <Pane prefHeight="26.0" prefWidth="554.0">
        <children>
            <Button alignment="CENTER" cancelButton="true" contentDisplay="CENTER" layoutY="-2.0" onAction="#onBackButtonClick" onMouseClicked="#onBackButtonClick" prefHeight="30.0" prefWidth="100.0" text="Wstecz" />
        </children>
    </Pane>
</VBox>
